{
  "tools": [
    {
      "name": "load_disease_targets",
      "description": "Load disease-associated therapeutic targets from OpenTargets database. Returns targets with genetic evidence (GWAS) and FDA-approved drugs.",
      "parameters": {
        "disease_name": {
          "type": "string",
          "description": "Name of the disease to query (e.g., 'rheumatoid arthritis', 'breast cancer')",
          "required": true
        },
        "use_api": {
          "type": "boolean",
          "description": "Whether to use OpenTargets API (true) or local files (false)",
          "required": false,
          "default": true
        },
        "attributes": {
          "type": "array",
          "description": "Datasource types to filter by (e.g., 'otGeneticsPortal', 'chembl')",
          "required": false,
          "default": ["otGeneticsPortal", "chembl"]
        }
      },
      "returns": {
        "type": "object",
        "properties": {
          "success": {"type": "boolean"},
          "targets": {"type": "array", "items": {"type": "string"}},
          "count": {"type": "integer"},
          "disease": {"type": "string"}
        }
      }
    },
    {
