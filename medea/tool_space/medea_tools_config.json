          "required": false,
          "default": "pft_leave_IMVigor210.pt"
        },
        "device": {
          "type": "string",
          "description": "Device for computation: 'cuda' or 'cpu' (default: cuda)",
          "required": false,
          "default": "cuda"
        },
        "threshold": {
          "type": "number",
          "description": "Response threshold (default: 0.5)",
          "required": false,
          "default": 0.5
        },
        "batch_size": {
          "type": "integer",
          "description": "Batch size for processing (default: 128)",
          "required": false,
          "default": 128
        }
      },
      "returns": {
        "type": "object",
        "properties": {
          "success": {"type": "boolean"},
          "responder": {"type": "boolean"},
          "cell_concepts": {"type": "array", "items": {"type": "array"}},
